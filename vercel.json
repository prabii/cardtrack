{
  "version": 2,
  "buildCommand": "cd cardtracker_pro && npm install && npm run build",
  "outputDirectory": "cardtracker_pro/build",
  "installCommand": "cd cardtracker_pro && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "cardtracker_pro/src/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
